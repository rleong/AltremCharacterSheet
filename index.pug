doctype html
html
	head
		link(rel="stylesheet" href="style.css")
	div.sheet-main

		// Tabs
		img(src="https://raw.githubusercontent.com/rleong/AltremCharacterSheet/main/Initials_Signature.png" alt="Property of Russell Leong" style="width:45px;height:45px;")
		input(type="radio" name="attr_tab" value="0" title="Everything" checked).sheet-tab-button.sheet-tab-button-all
		span.sheet-tab-label &nbsp;View All&nbsp;
		input(type="radio" name="attr_tab" value="1" title="No1").sheet-tab-button.sheet-tab-button1
		span.sheet-tab-label &nbsp;Combat&nbsp;
		input(type="radio" name="attr_tab" value="2" title="No2").sheet-tab-button.sheet-tab-button2
		span.sheet-tab-label &nbsp;Stats&nbsp;
		input(type="radio" name="attr_tab" value="3" title="No3").sheet-tab-button.sheet-tab-button3
		span.sheet-tab-label &nbsp;Abilities&nbsp;
		input(type="radio" name="attr_tab" value="4" title="No4").sheet-tab-button.sheet-tab-button4
		span.sheet-tab-label &nbsp;Spells&nbsp;
		input(type="radio" name="attr_tab" value="5" title="No5").sheet-tab-button.sheet-tab-button5
		span.sheet-tab-label &nbsp;Strings&nbsp;
		input(type="radio" name="attr_tab" value="6" title="No6").sheet-tab-button.sheet-tab-button6
		span.sheet-tab-label &nbsp;Character Details&nbsp;
		input(type="radio" name="attr_tab" value="7" title="No7").sheet-tab-button.sheet-tab-button7
		span.sheet-tab-label &nbsp;Inventory&nbsp;
		input(type="radio" name="attr_tab" value="8" title="No8").sheet-tab-button.sheet-tab-button8
		span.sheet-tab-label &nbsp;Notebook&nbsp;

		// Header
		hr
		div.sheet-header.sheet-div-center
			div.sheet-header-block
				div.sheet-undertext Player Name
				input(type="text" name="attr_player_name" style="width:130px;")
			div.sheet-header-block
				div.sheet-undertext Character Name
				input(type="text" name="attr_char_name" style="width:130px;")
			div.sheet-header-block
				div.sheet-undertext Race
				input(type="text" name="attr_race" style="width:130px;")
			div.sheet-header-block
				div.sheet-undertext.sheet-th-vert Total Level
				input(type="number" name="attr_lvl" value="1" min="1" step="1" max="20").sheet-numbox
			div.sheet-header-block
				div.sheet-undertext.sheet-th-vert Proficiency Bonus
				p(style="display:inline-block;") +
				input(type="text" name="attr_prof" value="(1 + ceil((@{lvl})/4))" disabled).sheet-numbox.sheet-sm-text

		// Body
		div.sheet-header-bottom-border

		// Tab 1
		div.sheet-tabbed-pane.sheet-tab.sheet-tab1

		// Tab 2
		div.sheet-tabbed-pane.sheet-tab.sheet-tab2
			div.sheet-header.sheet-div-center
				div.sheet-header-block
					div.sheet-undertext.sheet-th-vert Size
					select(name="attr_size").sheet-numbox.sheet-width-auto
						option(value="Tiny") Tiny
						option(value="Small") Small
						option(value="Medium") Medium
						option(value="Large") Large
						option(value="Huge") Huge
						option(value="Gargantuan") Gargantuan
				div.sheet-header-block
					div.sheet-undertext.sheet-th-vert Walking Speed
					input(type="number" name="attr_walk_speed" min="0" value="0").sheet-numbox
				div.sheet-header-block
					div.sheet-undertext.sheet-th-vert Climbing Speed
					input(type="number" name="attr_climb_speed" min="0" value="0").sheet-numbox
				div.sheet-header-block
					div.sheet-undertext.sheet-th-vert Crawling Speed
					input(type="number" name="attr_crawl_speed" min="0" value="0").sheet-numbox
				div.sheet-header-block
					div.sheet-undertext.sheet-th-vert Flying Speed
